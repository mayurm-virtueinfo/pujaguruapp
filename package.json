{
  "name": "pujaguruapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "ENVFILE=.env.development react-native run-android",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "ios": "ENVFILE=.env.development react-native run-ios --scheme 'PujaGuruApp-Development'",
    "ios:prod": "ENVFILE=.env.production react-native run-ios --scheme 'PujaGuruApp-Production'",
    "clean": "cd android && ./gradlew clean && cd ..",
    "apk": "ENVFILE=.env.development cd android && ./gradlew assembleRelease && cd ..",
    "apk:prod": "ENVFILE=.env.production cd android && ./gradlew assembleRelease && cd ..",
    "aab": "ENVFILE=.env.development cd android && ./gradlew bundleDebug && cd ..",
    "aab:prod": "ENVFILE=.env.production cd android && ./gradlew bundleRelease && cd ..",
    "firebase-emulator": "firebase emulators:start --only auth",
    "dev": "concurrently \"firebase emulators:start --only auth\" \"ENVFILE=.env.development react-native start\"",
    "ios:dev": "concurrently \"firebase emulators:start --only auth\" \"npm run ios\"",
    "lint": "eslint .",
    "test": "jest",
    "postinstall": "patch-package"
  },
